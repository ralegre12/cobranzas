{
  "name": "cobranzas-bircle-like",
  "version": "0.2.0",
  "private": true,
  "engines": {
    "node": ">=18.18 <21",
    "npm": ">=9"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "start:prod": "NODE_ENV=production node dist/main.js",
    "start:debug": "node --inspect=0.0.0.0:9229 dist/main.js",
    "lint": "eslint \"src/**/*.{ts,js}\"",
    "lint:fix": "eslint \"src/**/*.{ts,js}\" --fix",
    "format": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "check:types": "tsc -p tsconfig.json --noEmit",
    "typeorm": "node --require ts-node/register --require tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/shared/orm.datasource.ts",
    "migrate:run": "npm run typeorm -- migration:run",
    "migrate:revert": "npm run typeorm -- migration:revert",
    "migrate:show": "npm run typeorm -- migration:show",
    "seed": "ts-node -r tsconfig-paths/register ./scripts/seed.ts",
    "seed:templates": "ts-node -r tsconfig-paths/register scripts/seed-templates.ts",
    "db:gen": "typeorm-ts-node-commonjs migration:generate -d src/shared/orm.datasource.ts src/migrations/Change",
    "db:create": "typeorm-ts-node-commonjs migration:create src/migrations/Empty",
    "db:run": "typeorm-ts-node-commonjs migration:run -d src/shared/orm.datasource.ts",
    "db:revert": "typeorm-ts-node-commonjs migration:revert -d src/shared/orm.datasource.ts",
    "db:show": "typeorm-ts-node-commonjs migration:show -d src/shared/orm.datasource.ts",
    "db:log": "typeorm-ts-node-commonjs schema:log -d src/shared/orm.datasource.ts"
  },
  "dependencies": {
    "@fastify/multipart": "7.7.3",
    "@fastify/static": "^7.0.4",
    "@nestjs/axios": "^3.1.3",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "10.4.20",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "10.4.20",
    "@nestjs/platform-express": "10.4.20",
    "@nestjs/platform-fastify": "10.4.20",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^10.0.0",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.61.0",
    "cache-manager": "^7.2.0",
    "chrono-node": "^2.8.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "csv-parse": "^5.6.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.18",
    "dotenv": "^16.4.5",
    "fastify": "^4.29.1",
    "fastify-raw-body": "^4.3.0",
    "ioredis": "^5.8.1",
    "libphonenumber-js": "^1.12.24",
    "mercadopago": "^2.9.0",
    "multer": "1.4.5-lts.1",
    "nodemailer": "^6.10.1",
    "pg": "^8.11.5",
    "pino": "^9.3.2",
    "pino-http": "^10.3.0",
    "redis": "^5.8.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "timezone-support": "^3.1.0",
    "twilio": "^5.8.2",
    "typeorm": "^0.3.20",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "10.4.20",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.19.22",
    "@types/nodemailer": "^7.0.1",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
